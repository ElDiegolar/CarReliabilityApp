{
    "version": 2,
    "framework": "vue",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/*.js": {
            "runtime": "@vercel/node@2.15.10"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "VUE_APP_API_BASE_URL": "https://car-reliability-app.vercel.app"
    },
    "github": {
        "enabled": true
    },
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "nodeVersion": "18.x"
            }
        }
    ]
}