{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/server.js"
        }
    ],
    "functions": {
        "api/server.js": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}